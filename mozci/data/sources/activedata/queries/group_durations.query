from: unittest
groupby:
    - task.id
    - result.group
select:
    - {name: result_duration, value: result.duration, aggregate: sum}
limit: 100000
where:
    and:
        - ne: {result.status: "SKIP"}
        - in: {repo.changeset.id: {$eval: push_ids}}
        - ne: {result.duration: 0}
        - exists: result.group